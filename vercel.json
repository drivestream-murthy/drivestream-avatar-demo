{
  "version": 2,
  "routes": [
    { "src": "/(js|public)/(.*)", "dest": "/$1/$2" },
    { "src": "/styles\\.css", "dest": "/styles.css" },
    { "src": "/(.*\\.(?:js|css|png|jpg|jpeg|svg|ico|json|txt|html))", "dest": "/$1" },
    { "handle": "filesystem" },
    { "src": "/.*", "dest": "/index.html" }
  ]
}
